{
	"name": "oefenweb/cakephp-queue",
	"description": "A minimalistic Queue Plugin for CakePHP",
	"type": "cakephp-plugin",
	"keywords": [
		"cakephp", "queue"
	],
	"homepage": "http://github.com/Oefenweb/cakephp-queue",
	"license": "MIT",
	"authors": [
		{
			"name": "Oefenweb.nl BV team",
			"homepage": "https://github.com/Oefenweb/cakephp-queue/contributors"
		}
	],
	"require": {
		"php": ">=7.0.0",
        "cakephp/cakephp": "^3.5",
		"composer/installers": "~1.0"
	},
    "require-dev": {
        "phpunit/phpunit": "^5.7.14|^6.0"
    },
	"support": {
		"issues": "https://github.com/Oefenweb/cakephp-queue/issues",
		"source": "https://github.com/Oefenweb/cakephp-queue"
	},
    "autoload": {
        "psr-4": {
            "Queue\\": "src/",
            "Queue\\Test\\Fixture\\": "tests/Fixture/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Queue\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    },
	"extra": {
		"installer-name": "Queue"
	}
}
